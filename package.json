{
  "name": "git-submodule-sort",
  "displayName": "git submodule sort",
  "description": "Sort the submodule and make the submodule look clearer",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "activationEvents": [],
  "icon": "logo.png",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-submodule-sort.patch",
        "title": "Enable git submodule sort"
      }
    ],
    "configuration": {
      "title": "git submodule sort",
      "properties": {
        "git-submodule-sort.prefix": {
          "description": "submodule repo prefix",
          "type": "string",
          "default": "â”¡"
        }
      }
    }
  },
  "publisher": "R",
  "repository": "https://github.com/wolfsilver/git-submodule-sort",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "@babel/generator": "~7.14.5",
    "@babel/parser": "~7.14.7",
    "@babel/template": "~7.14.5",
    "@babel/traverse": "~7.14.7",
    "@babel/types": "~7.14.5"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.58.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
